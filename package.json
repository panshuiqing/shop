{
  "name": "shop",
  "version": "1.0.0",
  "description": "shop system",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "build": "rimraf dist && webpack --config config/webpack.prod.js",
    "build:electron.renderer": "webpack --config config/electron/electron.renderer.prod.js",
    "build:electron.main": "webpack --config config/electron/electron.prod.js",
    "build:electron": "npm run build:electron.renderer && npm run build:electron.main",
    "clear": "rimraf dist && rimraf build",
    "test": "electron build",
    "pack": "electron-packager build LiteMarket --icon=Logo.ico --overwrite --platform=win32 --arch=ia32 --prune=true --out=release "
  },
  "keywords": [
    "shop",
    "market"
  ],
  "author": "pansq",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.4.5",
    "@angular/compiler": "2.4.5",
    "@angular/core": "2.4.5",
    "@angular/forms": "2.4.5",
    "@angular/http": "2.4.5",
    "@angular/platform-browser": "2.4.5",
    "@angular/platform-browser-dynamic": "2.4.5",
    "@angular/router": "3.4.5",
    "core-js": "^2.4.1",
    "mysql": "^2.13.0",
    "primeng": "^2.0.3",
    "rxjs": "5.0.1",
    "sequelize": "^3.30.2",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/angular": "^1.5.16",
    "@types/angular-animate": "^1.5.5",
    "@types/angular-resource": "^1.5.6",
    "@types/angular-route": "^1.3.2",
    "@types/angular-sanitize": "^1.3.3",
    "@types/electron": "^1.4.33",
    "@types/mysql": "0.0.31",
    "@types/node": "^6.0.45",
    "@types/sequelize": "^4.0.46",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.4",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "electron": "^9.1.0",
    "electron-packager": "^8.5.2",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "http-server": "^0.9.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.1",
    "typescript": "^2.2.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-merge": "^3.0.0"
  }
}
